                    <file>${file.reference.jersey-common-2.26.jar}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
                <library dirs="200">
                    <file>${file.reference.jersey-container-servlet-2.26.jar}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
